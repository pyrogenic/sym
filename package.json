{
  "name": "sym",
  "private": true,
  "scripts": {
    "discojs": "yarn workspace discojs",
    "elephant": "yarn workspace @pyrogenic/elephant",
    "memo": "yarn workspace @pyrogenic/memo",
    "start": "npm-run-all --parallel 'memo watch' 'discojs build-dev --watch' 'elephant start'"
  },
  "dependencies": {
    "npm-run-all": "~4.1.5"
  },
  "workspaces": [
    "packages/*"
  ]
}
