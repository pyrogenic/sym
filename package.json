{
  "name": "sym",
  "private": true,
  "scripts": {
    "asset": "yarn workspace @pyrogenic/asset",
    "discojs": "yarn workspace discojs",
    "elephant": "yarn workspace @pyrogenic/elephant",
    "memo": "yarn workspace @pyrogenic/memo",
    "perl": "yarn workspace @pyrogenic/perl",
    "proon": "yarn workspace @pyrogenic/proon",
    "slide-grid": "yarn workspace @pyrogenic/slide-grid",
    "spelling-sea": "yarn workspace @pyrogenic/spelling-sea",
    "testbed": "yarn workspace @pyrogenic/testbed",
    "-test": "npm-run-all --parallel 'asset test'",
    "-test-watch": "npm-run-all --parallel 'asset test --watchAll'",
    "toed": "yarn workspace @pyrogenic/toed",
    "start": "npm-run-all --parallel 'asset watch' 'memo watch' 'perl watch' 'slide-grid watch'"
