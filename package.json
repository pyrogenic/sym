    "**/babel-jest": "^26.6.0",
    "**/bootstrap": "^5",
    "**/jest": "26.6.0",
    "**/lodash": "^4.17.12",
    "**/minimist": "^1.2.6",
    "**/react-bootstrap": "^2.1",
    "**/react": "~17",
    "**/typescript": "~4.6.4"
  },
  "workspaces": [
    "packages/*"
  ]
